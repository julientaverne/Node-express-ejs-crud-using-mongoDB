<% i_communes.forEach((item) => { %>

<!--begin::Item-->
<a href="pages/social/feeds.html" class="parent-hover cursor-pointer d-flex align-items-center flex-md-row-fluid py-lg-2 px-0">
    <!--begin::Symbol-->
    <div class="symbol symbol-35px symbol-lg-40px me-3">
        <span class="symbol-label rounded-1" style="background-color: #ffffff">
            <% if (item && item.blason) { %>
                <img src="media/app/blasons/<%= item.blason %>" class="mw-25px" alt="" />
            <% } else { %>
                <img src="media/logos/zi_nobg.png" class="mw-25px" alt="" />
            <% } %>
        </span>
    </div>
    <!--end::Symbol-->
    <!--begin::Info-->
    <div class="d-flex justify-content-center flex-column">
        <span class="fw-bold text-gray-800 parent-hover-primary fs-5 mb-1"><%= item.nom_standard %></span>
        <span class="text-gray-500 fw-semibold d-block fs-7"><%= item.code_postal %></span>
    </div>
    <!--end::Info-->
</a>
<!--end::Item-->

<% }); %>

