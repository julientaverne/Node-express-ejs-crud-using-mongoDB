<% i_communes.forEach((item, index) => { %>

<!--begin::Item-->
<div class="d-flex mb-7">
    <!--begin::Symbol-->
    <div class="symbol symbol-100px symbol-2by3 flex-shrink-0 me-4">
        <img src="media/app/communes/<%= item.code_insee %>/01.jpeg" class="mh-100" alt="" onerror="this.src='media/logos/zi_nobg.png';this.style='width: auto; height: 100px; object-fit: contain;'" />
    </div>
    <!--end::Symbol-->
    <!--begin::Section-->
    <div class="d-flex align-items-center flex-wrap flex-grow-1 mt-n2 mt-lg-n1">
        <!--begin::Title-->
        <div class="d-flex flex-column flex-grow-1 my-lg-0 my-2 pe-3">
            <a href="#" class="fs-5 text-gray-800 text-hover-primary fw-bold"><%= item.nom_standard %></a>
            <span class="text-gray-500 fw-semibold fs-7 my-1"><%= item.dep_code %> <%= item.dep_nom %></span>
            <span class="text-gray-500 fw-semibold fs-7 my-1">
                <span class="badge badge-success">OK 01</span>
                <span class="badge badge-success">OK 02</span>
            </span>
            <span class="text-gray-500 fw-semibold fs-7">
                <span class="badge badge-danger">KO 01</span>
            </span>
        </div>
        <!--end::Title-->
        <!--begin::Info-->
        <div class="text-end py-lg-0 py-2">
            <span class="text-gray-500 fs-7 fw-semibold d-block my-1">
                <span class="badge badge-circle badge-primary">
                    <%= index+1 %>
                </span>
            </span>
            <span class="text-gray-800 fw-bolder fs-4"> <i class="fa fa-star-half-alt me-1 text-warning fs-5"></i>
                <span class="text-gray-800 fw-bold fs-5">4.2</span></span>
            <span class="text-gray-500 fs-7 fw-semibold d-block my-1">Sales</span>
            <span class="text-gray-800 fw-semibold fs-7 my-1"><%= item.population %> hab.</span>
            
           

           

        </div>
        <!--end::Info-->
    </div>
    <!--end::Section-->
</div>
<!--end::Item-->

<% }); %>